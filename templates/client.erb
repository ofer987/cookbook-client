server {
  listen 443 ssl;
  ssl_certificate <%= @ssl_certificate_path %>;
  ssl_certificate_key <%= @ssl_certificate_key %>;
  server_name <%= @server_names %>;

  root <%= @path %>;
}

server {
  listen 80;
  listen [::]:80;
  server_name <%= @server_names %>;

  # return 301 https://$host$request_uri;
  root <%= @path %>;
}

